# API Configuration
# Copy this file to .env and fill in your actual API keys

# Replicate API (for various image editing models)
# Get your key from: https://replicate.com/account/api-tokens
# REPLICATE_API_KEY=your_replicate_api_key_here

# OpenAI API (for DALL-E image editing)
# Get your key from: https://platform.openai.com/api-keys
# OPENAI_API_KEY=your_openai_api_key_here

# Stability AI API (for Stable Diffusion)
# Get your key from: https://platform.stability.ai/account/keys
# STABILITY_API_KEY=your_stability_api_key_here

# Google AI API (for Gemini / Nano Banana - FREE!)
# Get your key from: https://aistudio.google.com/apikey
# GOOGLE_API_KEY=your_google_api_key_here

# Default API provider to use (google, replicate, openai, or stability)
DEFAULT_API_PROVIDER=google
